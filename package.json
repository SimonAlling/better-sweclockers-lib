{
  "name": "better-sweclockers-lib",
  "version": "1.1.2",
  "description": "Used in Better SweClockers.",
  "keywords": [
    "SweClockers",
    "library"
  ],
  "author": {
    "name": "Simon Alling",
    "email": "alling.simon@gmail.com",
    "url": "https://simonalling.se"
  },
  "license": "MIT",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "scripts": {
    "lint": "tslint --project .",
    "build": "npm run clean && cross-env TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack && mv src/*.d.ts dist",
    "clean": "rm -rf dist/*",
    "prepublishOnly": "npm run verify",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm test"
  },
  "sideEffects": false,
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/__tests__/.+\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/node-sass": "^3.10.32",
    "@types/webpack": "^4.4.23",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "node-sass-utils": "^1.1.2",
    "prettier": "^1.15.3",
    "renamer": "^1.1.1",
    "sass-loader": "^7.1.0",
    "to-string-loader": "^1.1.5",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@typed/compose": "^1.0.0",
    "bbcode-tags": "^1.0.0",
    "highlight-mistakes": "^1.0.0",
    "sweclockers-writing-rules": "github:SimonAlling/sweclockers-writing-rules",
    "ts-type-guards": "^0.6.1"
  }
}
